{
  "expo": {
    "name": "manito-app",
    "slug": "manito-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "manito",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cl.manito.app",
      "associatedDomains": [
        "applinks:auth.manito.cl",
        "applinks:app.manito.cl",
        "applinks:auth.manito.cl",
        "applinks:app.manito.cl"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "manito-auth",
            "CFBundleURLSchemes": [
              "manito"
            ]
          },
          {
            "CFBundleURLName": "manito-auth",
            "CFBundleURLSchemes": [
              "manito"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "cl.manito.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "auth.manito.cl"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "manito"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "40fe168f-16ee-47d9-95ab-4d21f2733ff8"
      }
    }
  }
}
